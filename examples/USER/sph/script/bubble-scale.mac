/*
k :: heat conductivity
cp :: heat capacity
rho :: density
alpha: k/(rho*cp) :: thermal diffusivity
H                 :: latent heat
dT                :: Tv-Tsat
Ja:               :: Jacob number, (dT*cp*rho_l)/(rho_g*H)
*/

kill(all);
d: 3;
eq1: dN = dE/Lp;
eq2: dE = S*dt/R;
eq3: S=R^(d-1);
eq4: dN = R^(d-1)*dR;

solve([eq1, eq2, eq3, eq4], [dE, S, dR, dN]);